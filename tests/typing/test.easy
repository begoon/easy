PROGRAM Typing:
  DECLARE s STRING;
  DECLARE (i, j, v) INTEGER;
  DECLARE r REAL;
  FUNCTION S() STRING: RETURN "abc"; END FUNCTION S;
  FUNCTION I() INTEGER: RETURN 123; END FUNCTION I;
  FUNCTION R() REAL: RETURN 567.89; END FUNCTION R;
  FUNCTION B() BOOLEAN: RETURN TRUE; END FUNCTION B;

  OUTPUT S();
  OUTPUT I();
  OUTPUT R();
  OUTPUT B();

  SET s := S() || "xzy";
  OUTPUT s;

  SET v := i + j;
  OUTPUT v;

  SET v := 1 + 2.1;
  OUTPUT v;
END PROGRAM Typing;
