PROGRAM String:
  DECLARE s STRING;
  DECLARE b BOOLEAN;
  SET s := "abcXYZ";
  SET b := s = "abcXYZ";
  SET b := "abcXYZ" = s;
  SET b := "abcXYZ" = "abcXYZ";
  SET b := s = s;

  SET s := "abcXYZ";
  SET b := s <> "abcXYZ";
  SET b := "abcXYZ" <> s;
  SET b := "abcXYZ" <> "abcXYZ";
  SET b := s <> s;

  OUTPUT SUBSTR("12345", 1, 2);
  OUTPUT SUBSTR(s, 1, 2);
  OUTPUT "abc" || SUBSTR(s, 3, 2);
  EXIT;
END PROGRAM String;
