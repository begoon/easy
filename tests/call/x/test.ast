PROGRAM Call
    Variables:
        x: INTEGER
    Subroutines:
        PROCEDURE a(x)
            Statements:
                OUTPUT('A', ||:('si', 'a(): ', x))
        FUNCTION b(INTEGER x) : INTEGER
            Statements:
                RETURN (x + 1)
    Statements:
        CALL a(100)
        SET x := b(100)
        OUTPUT('i', x)
        EXIT
