PROGRAM Test
    Variables:
        a: INTEGER
    Statements:
        SET a := 100
        SELECT TRUE
            CASE (a < 0):
                Statements:
                    OUTPUT('A', 'a < 0')
            CASE (a = 0):
                Statements:
                    OUTPUT('A', 'a = 0')
            OTHERWISE:
                Statements:
                    OUTPUT('A', 'otherwise')
        END SELECT
        EXIT
