//easy:flags index_check=0
PROGRAM ArrayUnchecked:
  DECLARE b ARRAY [1:2] OF INTEGER;
  DECLARE m ARRAY [1:2] OF ARRAY [1:4] OF INTEGER;
  DECLARE c ARRAY [1:2] OF ARRAY [1:4] OF ARRAY [1:8] OF INTEGER;
  DECLARE x ARRAY [1:2] OF ARRAY [1:4] OF STRUCTURE FIELD i IS INTEGER END STRUCTURE;
  DECLARE y ARRAY [1:2] OF ARRAY [1:4] OF STRUCTURE FIELD i IS INTEGER, FIELD t IS ARRAY [1:4] OF INTEGER END STRUCTURE;

  SET b[1] := 42;
  OUTPUT b[1];

  SET m[2][3] := 99;
  OUTPUT m[2][3];

  SET c[1][2][3] := 123;
  OUTPUT c[1][2][3];

  SET x[1][2].i := 456;
  OUTPUT x[1][2].i;
  
  SET y[2][3].i := 789;
  OUTPUT y[2][3].i;
END PROGRAM ArrayUnchecked;
